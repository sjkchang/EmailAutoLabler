{
    "manifest_version": 3,
    "name": "Gmail Labeler",
    "description": "Automatically label Gmail emails.",
    "version": "1.0",
    "permissions": ["identity", "storage", "activeTab", "scripting"],
    "host_permissions": ["https://mail.google.com/*"],
    "background": {
      "service_worker": "service-worker.js"
    },
    "oauth2": {
      "client_id": "162618327556-5kb6gtv4jnu0otqj91m4irhevreuuhap.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/gmail.modify", "https://www.googleapis.com/auth/userinfo.profile", "https://www.googleapis.com/auth/userinfo.email"]
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "label.png"
    },
    "key": ""
  }
  